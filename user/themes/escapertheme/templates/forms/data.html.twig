{% set myVal = 1 %}
{% for field in form.fields %}
{% block field %}
    {% if (field.label != "Captcha") %}
    <div class="uk-width-1-1 uk-panel uk-panel-box uk-margin-bottom panelcolor{{myVal}}">
    {% block field_label %}<strong>{{ field.label }}</strong>{% endblock %}: {% block field_value %}{{ string(form.value(field.name)|e|nl2br) }}{% endblock %}
    </div>
    {% endif %}
{% endblock %}
    {% set myVal = myVal + 1 %}
{% endfor %}
